        <JSR223Listener guiclass="TestBeanGUI" testclass="JSR223Listener" testname="LogAllRequestandBody" enabled="true">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import java.util.Date;
import java.text.SimpleDateFormat;
def lock = new Object();
synchronized (lock) {
	try{
	  var message = &quot;&quot;;
	  var currentUrl = sampler.getUrl();
	  var httpMethod = sampler.getMethod();
	  
	  message +=  &quot;. URL = &quot; +httpMethod + &quot; &quot; +currentUrl;
	  message += &quot;\n--data &quot; + sampler.getArguments();
	  message += &quot;\n--requestHeader = &quot; + prev.getRequestHeaders();
	  message += &quot;\n--responseCode = &quot; + prev.getResponseCode();
	  message += &quot;\n--responseData = &quot; + prev.getResponseDataAsString();  
	  message += &quot;\n--promoCode = &quot; + vars.get(&quot;promoCode&quot;);
	  message += &quot;\n--email = &quot; + vars.get(&quot;EMAIL_SIGNUP&quot;);
	  message += &quot;\n--basketId = &quot; + vars.get(&quot;basketId&quot;);
	  message += &quot;\n--token = &quot; + vars.get(&quot;TOKEN&quot;);
	  message += &quot;\n--machineIP = &quot; + vars.get(&quot;machineIP&quot;);
	  
	  if(vars.get(&quot;isChosenDAZN&quot;).equals(&quot;1&quot;)){
	  	message += &quot;\n--dazn = &quot; + vars.get(&quot;dazn&quot;);	  	
	  }
	  message += &quot;\n--device = &quot; + vars.get(&quot;device&quot;);
	  if(vars.get(&quot;isChosenNetflix&quot;).equals(&quot;1&quot;)){
	  	message += &quot;\n--netflixPackage = &quot; + vars.get(&quot;netflixPackage&quot;);
	  }
	  message += &quot;\n--receiver = &quot; + vars.get(&quot;receiver&quot;);
	  if((vars.get(&quot;receiver&quot;).equals(&quot;QOverIP&quot;) &amp;&amp; vars.get(&quot;quality&quot;).equals(&quot;HD&quot;)) || (!vars.get(&quot;receiver&quot;).equals(&quot;QOverIP&quot;) &amp;&amp; !vars.get(&quot;quality&quot;).equals(&quot;HD&quot;))){
	  	message += &quot;\n--quality = &quot; + vars.get(&quot;quality&quot;);
	  }
	  if(vars.get(&quot;receiver&quot;).equals(&quot;Multiscreen&quot;)){
	  	message += &quot;\n--additionalHardware = &quot; + vars.get(&quot;additionalHardware&quot;);	  	
	  }
	  message += &quot;\n--bundle = &quot; + vars.get(&quot;bundle&quot;);

	  long expectedDuration = 1000L;
	  long actualDuration = sampleResult.getTime();
	  long time = sampleResult.getStartTime();
	  Date d = new Date(time);
	  SimpleDateFormat sdf = new SimpleDateFormat(&quot;dd/MM/yyyy HH:mm:ss&quot;);
	  String startTime = sdf.format(d);
	  message += &quot;\n--responseTime = &quot; + String.valueOf(actualDuration) + &quot;ms&quot;;
	  message += &quot;\n--startTime = &quot; + startTime;
	  message += &quot;\n&quot;;
	  message += &quot;\n&quot;;
	  message += &quot;\n-------Logging ends here-----------&quot;;
	
	  if(!sampleResult.isSuccessful() || (actualDuration &gt; expectedDuration) &amp;&amp; ${__P(isSavedLog,true)}){
	      log.error(message);
	  }

	}catch(Exception err){
	  log.error(err.toString());
	}
}</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223Listener>
