          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="cutomizeTestData" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import java.util.*;

// Clear values of parameters
vars.put(&quot;EMAIL_SIGNUP&quot;,&quot;&quot;);
vars.put(&quot;basketId&quot;,&quot;&quot;);
vars.put(&quot;accessToken&quot;,&quot;&quot;);
vars.put(&quot;selectTrendsport&quot;, new Random().nextBoolean().toString());

// Generate x-sky-tracing-session &amp; X-Sky-RequestId
vars.put(&quot;skyTracingSession&quot;, UUID.randomUUID().toString())
vars.put(&quot;skyRequestId&quot;, UUID.randomUUID().toString())

def defaultProducts = vars.get(&quot;defaultProducts&quot;);
def productCodes = vars.get(&quot;productCodes&quot;);

String[] productList = productCodes.split(&quot;,&quot;);
productList = Arrays.copyOf(productList, productList.length +1);
productList[productList.length - 1] = defaultProducts

String selectedProduct = productList[new Random().nextInt(productList.length - 1)]
vars.put(&quot;selectedProduct&quot;, selectedProduct)
String reSelectedProduct = productList[new Random().nextInt(productList.length - 1)];
if(reSelectedProduct != defaultProducts){
	vars.put(&quot;reSelectedProduct&quot;, reSelectedProduct)
}else{
	vars.put(&quot;reSelectedProduct&quot;, selectedProduct)
}</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
